{
	"schemaVersion": 1,
	"id": "itemfig",
	"version": "${version}",
	"name": "ItemFig",
	"description": "Simple mod that allows for config of item parameters.",
	"authors": [
		"CrazedAerialCable"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/itemfig/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"io.github.hornster.itemfig.ItemFig"
		],
		"client": [
			"io.github.hornster.itemfig.ItemFigClient"
		],
		"fabric-datagen": [
			"io.github.hornster.itemfig.ItemFigDataGenerator"
		]
	},
	"mixins": [
		"itemfig.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}