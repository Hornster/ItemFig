{
	"schemaVersion": 1,
	"id": "itemfig",
	"version": "${version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_authors}"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "${mod_license}",
	"icon": "assets/itemfig/itemfigicon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"io.github.hornster.itemfig.ItemFig"
		],
		"client": [
			"io.github.hornster.itemfig.ItemFigClient"
		],
		"fabric-datagen": [
			"io.github.hornster.itemfig.ItemFigDataGenerator"
		]
	},
	"mixins": [
		"itemfig.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "~1.19.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}